{
  "name": "sds",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--disable-warning=ExperimentalWarning' webpack serve --config webpack.config.js --mode development",
    "build": "NODE_OPTIONS='--disable-warning=ExperimentalWarning' webpack --config webpack.config.js --mode production",
    "build:analyze": "NODE_OPTIONS='--disable-warning=ExperimentalWarning' webpack --config webpack.config.js --mode production --env analyze",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "element-plus": "^2.4.4",
    "pinia": "^3.0.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.7",
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@vue/compiler-sfc": "^3.4.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/vue3-jest": "^29.2.6",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.19.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.15.0",
    "unplugin-element-plus": "^0.2.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.3.0",
    "vue-tsc": "^2.0.6",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "overrides": {
    "sourcemap-codec": "@jridgewell/sourcemap-codec@^1.5.0",
    "rimraf": "^5.0.0",
    "glob": "^10.4.0"
  },
  "lint-staged": {
    "*.{vue,ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{vue,ts,tsx}": [
      "vue-tsc --noEmit"
    ]
  }
}
